{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "agent_ui (Flutter Chrome)",
            "program": "lib/main.dart",
            "cwd": "ui/agent_ui",
            "deviceId": "chrome",
            "request": "launch",
            "type": "dart",
            "toolArgs": [
                "--web-port=33931",
                "--web-browser-flag=--disable-web-security",
                "--web-browser-flag=--user-data-dir=/tmp/chrome_dev_test"
            ]
        }
    ]
}